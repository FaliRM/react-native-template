{
  "presets": ["module:metro-react-native-babel-preset"],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "_assets": "./src/assets",
          "_components": "./src/components",
          "_config": "./src/config",
          "_context": "./src/context",
          "_hooks": "./src/hooks",
          "_i18n": "./src/i18n",
          "_navigation": "./src/navigation",
          "_screens": "./src/screens",
          "_services": "./src/services",
          "_styles": "./src/styles",
          "_types": "./src/types",
          "_utils": "./src/utils"
        }
      }
    ]
  ]
}
